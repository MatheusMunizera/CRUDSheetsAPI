{
    "version": 2,
    "builds":[
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src" : "/(.*)",
            "dest": "src/main.ts",
            "headers": { "Access-Control-Allow-Origin":  "https://localhost:4200, https://submit-to-sheets.matheusmuniz.dev" },
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]


}