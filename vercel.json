{
    "version": "0.0.1",
    "env" : {
        "SHEET_URI" : "@sheet_uri"
    },
    "builds":[
        {
            "src": "src/main.ts",
            "user": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src" : "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}